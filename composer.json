{
    "name": "syntro/silverstripe-elemental-elementals",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Matthias Leutenegger",
            "email": "hello@syntro.ch"
        }
    ],
    "require": {
        "dnadesign/silverstripe-elemental": "^4",
        "gorriecoe/silverstripe-link": "^1",
        "gorriecoe/silverstripe-linkfield": "^1",
        "silverstripe/cms": "^4",
        "silverstripe/framework": "^4",
        "syntro/silverstripe-elemental-baseitem": "^1",
        "unclecheese/display-logic": "^2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "syntro/silverstripe-phpstan": "^1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Syntro\\SilverstripeElementalElementals\\": "src/",
            "Syntro\\SilverstripeElementalElementals\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/css"
        ],
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}